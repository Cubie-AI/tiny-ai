{
  "name": "@cubie-ai/tiny-ai",
  "version": "0.0.1",
  "description": "A tiny ai agent framework built by Cubie AI.",
  "license": "ISC",
  "author": "0xSoc <contact@cubie.fun> (https://cubie.fun)",
  "homepage": "https://cubie.fun",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cubie-AI/cubie-agi"
  },
  "bugs": {
    "url": "https://github.com/Cubie-AI/cubie-agi/issues"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "cubie",
    "ai-framework",
    "cubie-ai",
    "agent-framework",
    "tiny-ai"
  ],
  "scripts": {
    "build": "rm -rf dist/ && tsc",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "dev": "node --import tsx --watch -r ts-node/register src/index.ts",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.14.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "ai": "^4.3.9"
  }
}
